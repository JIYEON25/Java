package jy.java.exam15;

public class Account {
	private String name;
	private long balance;
	
	Account() { 
		
	}
	Account(String name) { 
		this.name = name;
	}
	
	public String getName() {
		return name;
	}
	
	public long getBalance(){
		return balance;
		}

	
	
	public Account(String name, long balance) {
		super();
		this.name = name;
		this.balance = balance;
	}
	
	
	
	
}
