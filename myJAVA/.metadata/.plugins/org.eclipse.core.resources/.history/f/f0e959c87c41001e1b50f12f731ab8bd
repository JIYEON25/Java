package jy.java.exam19;

public class HashcodeEx {
	public static void main(String[] args) {
		String str = new String("Test");
		String str2 = new String("Test");
		System.out.println("str / str2 주소값 비교: " + (str == str2));
		System.out.println("str / str2 해시코드 비교: " + (str.hashCode() == str2.hashCode()));
		System.out.println("str hashcode : " + str.hashCode());
		System.out.println("str2 hashcode : " + str2.hashCode());
		
		// 객체의 해시코드는?
		HashcodeEx test1 = new HashcodeEx();
		HashcodeEx test2 = new HashcodeEx();
		System.out.println("test1/ test2 address : " + (test1 == test2));
		System.out.println("test1/test2 hashcode : " + (test1.hashCode() == test2.hashCode()));
		System.out.println("test1 hashcode : " + test1.hashCode());
		System.out.println("test2 hashcode : " + test2.hashCode());
		
		
		
	}
	
	
	
}
